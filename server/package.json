{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "index.ts",
  "type": "module",
  "engines": {
    "node": "24.4.1"
  },
  "imports": {
    "#*": "./src/*"
  },
  "scripts": {
    "start": "node index.ts",
    "dev": "node --env-file=.env --watch src/index.ts",
    "lint": "eslint . --fix",
    "test": "node --test tests/*.test.ts",
    "test:watch": "node --test --watch tests/*.test.ts",
    "test:coverage": "node --experimental-test-coverage --test tests/*.test.ts",
    "docker:start": "docker-compose -f docker-compose.yml -f docker-compose.prod.yml up --build",
    "docker:serve": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml up --build",
    "dockers:exec:api": "docker exec -it kwamsc-api /bin/sh",
    "dockers:exec:firebase": "docker exec -it firebase-emulator /bin/sh",
    "firebase-export-seed": "firebase emulators:export seed",
    "firebase:emulator-start": "firebase emulators:start --import=seed"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@eslint/js": "^9.12.0",
    "@types/node": "^20.16.11",
    "@types/sinon": "17.0.4",
    "@types/supertest": "6.0.2",
    "eslint": "9.13.0",
    "eslint-config-prettier": "^9.1.0",
    "globals": "^15.11.0",
    "pino-pretty": "^13.0.0",
    "prettier": "3.6.2",
    "sinon": "21.0.0",
    "supertest": "^7.1.4",
    "typescript": "5.5.4",
    "typescript-eslint": "8.11.0"
  },
  "dependencies": {
    "@fastify/cors": "11.0.1",
    "@fastify/helmet": "13.0.1",
    "@fastify/rate-limit": "10.3.0",
    "@fastify/swagger": "^9.5.1",
    "@fastify/swagger-ui": "5.2.3",
    "fastify": "5.4.0",
    "firebase": "12.0.0",
    "firebase-admin": "13.4.0"
  }
}