{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "engines": {
    "node": "22.13.0"
  },
  "imports": {
    "#*": "./src/*"
  },
  "scripts": {
    "build": "tsc -p .",
    "start": "node build/index.js",
    "dev": "tsx --env-file=.env --watch src/index.ts",
    "lint": "eslint . --fix",
    "test": "node --import tsx --test tests/*.test.ts",
    "test:watch": "node --import tsx --test --watch tests/*.test.ts",
    "test:coverage": "node --import tsx --experimental-test-coverage --test tests/*.test.ts",
    "docker:start": "docker-compose -f docker-compose.yml -f docker-compose.prod.yml up --build",
    "docker:serve": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml up --build",
    "dockers:exec:api": "docker exec -it kwamsc-api /bin/sh",
    "dockers:exec:firebase": "docker exec -it firebase-emulator /bin/sh",
    "firebase-export-seed": "firebase emulators:export seed",
    "firebase:emulator-start": "firebase emulators:start --import=seed"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@eslint/js": "^9.12.0",
    "@types/node": "^20.16.11",
    "@types/supertest": "6.0.2",
    "eslint": "9.13.0",
    "eslint-config-prettier": "^9.1.0",
    "globals": "^15.11.0",
    "pino-pretty": "^11.3.0",
    "prettier": "3.3.3",
    "supertest": "^7.0.0",
    "tsx": "^4.19.2",
    "typescript": "5.5.4",
    "typescript-eslint": "8.11.0"
  },
  "dependencies": {
    "@fastify/cors": "10.0.1",
    "@fastify/helmet": "13.0.0",
    "@fastify/rate-limit": "10.2.1",
    "@fastify/swagger": "^9.4.0",
    "@fastify/swagger-ui": "5.2.0",
    "fastify": "5.2.0",
    "firebase": "11.1.0",
    "firebase-admin": "13.0.2"
  }
}